{
  "name": "unobtainium",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "node dist/server/server.js",
    "serve": "ng serve --configuration development",
    "build": "ng build --configuration production && tsc",
    "tsc": "tsc",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
