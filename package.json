    "@angular/forms": "^19.2.5",
    "@angular/material": "^19.2.7",
    "@angular/platform-browser": "^19.2.5",
    "@angular/platform-browser-dynamic": "^19.2.5",
    "@angular/router": "^19.2.5",
    "@tailwindcss/postcss": "^4.0.12",
    "cors": "^2.8.5",
    "express": "^4.21.2",
    "fs": "^0.0.1-security",
    "googleapis": "^146.0.0",
    "node-cache": "^5.1.2",
